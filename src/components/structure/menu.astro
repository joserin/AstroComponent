---
interface Props {
    class?: string;
    [x: string]: any;
}
const defaultClass='default';
const { class: className = defaultClass, ...rest } = Astro.props;
---

<ul {...rest} class={className}>
    <slot/>
</ul>

<style>
    .default{
        background-color:  rgb(158, 54, 196);
        width: fit-content;
        padding: 0.5rem;
        border-radius: 0.5rem;
        display: flex;
        flex-direction: row;
        gap: 1.25rem;
        justify-content: center;
        align-items: center;
    }
</style>